<p id="notice"><%= notice %></p>

<h1>Listing Proveedors</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Razon social</th>
      <th>Rut</th>
      <th>Domicilio</th>
      <th>Telegono1</th>
      <th>Telefono2</th>
      <th>Web</th>
      <th>Mail</th>
      <th>Descripcion</th>
      <th>Comuna</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proveedors.each do |proveedor| %>
      <tr>
        <td><%= proveedor.nombre %></td>
        <td><%= proveedor.razon_social %></td>
        <td><%= proveedor.rut %></td>
        <td><%= proveedor.domicilio %></td>
        <td><%= proveedor.telegono1 %></td>
        <td><%= proveedor.telefono2 %></td>
        <td><%= proveedor.web %></td>
        <td><%= proveedor.mail %></td>
        <td><%= proveedor.descripcion %></td>
        <td><%= proveedor.comuna %></td>
        <td><%= link_to 'Show', proveedor %></td>
        <td><%= link_to 'Edit', edit_proveedor_path(proveedor) %></td>
        <td><%= link_to 'Destroy', proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proveedor', new_proveedor_path %>
